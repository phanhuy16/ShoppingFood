@model MomoExecuteResponseModel

@{
	ViewData["Title"] = "Kết quả thanh toán";
}

<!-- Single Page Header start -->
<div class="container-fluid page-header py-5">
	<h1 class="text-center text-white display-6">Payment</h1>
	<ol class="breadcrumb justify-content-center mb-0">
		<li class="breadcrumb-item"><a href="#">Home</a></li>
		<li class="breadcrumb-item"><a href="#">Pages</a></li>
		<li class="breadcrumb-item active text-white">Payment</li>
	</ol>
</div>
<!-- Single Page Header End -->

<div class="container-fluid py-5">
	<div class="container py-5">
		<h5>Thanh toán Momo thành công với: </h5>
		<h6>Mã đơn hàng: @Model.OrderId</h6>
		<h6>Thông tin đơn hàng: @Model.OrderInfo</h6>
		<h6>Tổng tiền: @Model.Amount</h6>
		<h5>Xem <a asp-controller="Account" asp-action="Profile">Lịch sử đơn hàng đã đặt</a></h5>
	</div>
</div>
